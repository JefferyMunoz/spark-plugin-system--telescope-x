{
  "name": "spark-plugin-system--telescope-x",
  "pluginName": "闪搭X",
  "author": "ssshuai",
  "version": "1.6.2",
  "description": "闪搭X - 极简开发者工具箱",
  "main": "dist/index.html",
  "preload": "public/preload.cjs",
  "logo": "logo.png",
  "pluginSetting": {
    "height": 665,
    "width": 845
  },
  "pluginType": "system",
  "entry": "main.cjs",
  "repo": "git@github.com:JefferyMunoz/spark-plugin-system--telescope-x.git",
  "runtimeEnv": {
    "node": {
      "dependencies": {}
    }
  },
  "features": [
    {
      "code": "telescope-x",
      "explain": "闪搭X",
      "cmds": [
        {
          "type": "over",
          "label": "闪搭X",
          "match": "/^sd$/i"
        },
        {
          "type": "over",
          "label": "闪搭X",
          "match": "/^sdx$/i"
        },
        "sd",
        "sdx",
        "闪搭",
        "闪搭X"
      ]
    },
    {
      "code": "quick-transform",
      "explain": "闪搭X",
      "cmds": [
        {
          "type": "over",
          "label": "闪搭X",
          "match": "/.+/"
        }
      ]
    }
  ]
}